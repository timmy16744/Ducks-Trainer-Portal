{
  "name": "ducks-trainer-portal",
  "version": "1.0.0",
  "description": "Comprehensive coaching portal with WGER exercise integration",
  "scripts": {
    "dev": "concurrently --names \"BACKEND,FRONTEND\" --prefix-colors \"blue,green\" \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && python app.py",
    "start:frontend": "cd frontend && npm start",
    "install:all": "cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "build": "cd frontend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "private": true,
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "immer": "^10.1.1",
    "react-beautiful-dnd": "^13.1.1",
    "recharts": "^3.1.0"
  }
}
